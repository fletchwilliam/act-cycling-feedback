# Database connection
DATABASE_URL=postgresql://username:password@localhost:5432/cycling_feedback

# Server settings
PORT=3000
NODE_ENV=development

# CORS settings (comma-separated origins, or * for all)
CORS_ORIGIN=http://localhost:8080,http://localhost:3000

# Admin settings (change these in production!)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=changeme

# Image storage (local or s3)
STORAGE_TYPE=local
# S3 settings (if using S3)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=ap-southeast-2
# S3_BUCKET=cycling-feedback-images
